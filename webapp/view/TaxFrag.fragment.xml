<core:FragmentDefinition controllerName="it.orogel.calcinteressisoci.controller.Home"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    xmlns="sap.ui.table">
    <m:Dialog id="_IDGenDialog1" title="{i18n>tax}" contentHeight="400px" contentWidth="550px">
        <m:content>

            <Table id="taxTable" rows="{path: 'taxModel>/'}" selectionMode="None" selectionBehavior="Row" rowSelectionChange=".onSelRow" alternateRowColors="true" visibleRowCount="10" paste="onPaste">
                <extension>
                    <m:OverflowToolbar id="_IDGenOverflowToolbar1" style="Clear">
                        <m:ToolbarSpacer id="_IDGenToolbarSpacer1"/>
                        <m:Button id="_IDGenButton3" icon="sap-icon://add" press="onAddTaxRows"/>
                        <m:Button id="_IDGenButton4" icon="sap-icon://delete" press="onDeleteTaxRows"/>
                    </m:OverflowToolbar>
                </extension>
                <columns>
                    <Column id="_IDGenColumn1" width="10em" sortProperty="ZZ_DATA_DA" filterProperty="ZZ_DATA_DA">
                        <m:Label id="_IDGenLabel1" text="{i18n>ZZ_DATA_DA}"/>
                        <template>
                            <m:DatePicker id="_IDGenDatePicker45" value="{path: 'taxModel>ZZ_DATA_DA'}" displayFormat="dd.MM.yyyy" valueFormat="yyyy-MM-dd"/>
                        </template>
                    </Column>
                    <Column id="_IDGenColumn2" width="10em" sortProperty="ZZ_DATA_A" filterProperty="ZZ_DATA_A">
                        <m:Label id="_IDGenLabel2" text="{i18n>ZZ_DATA_A}"/>
                        <template>
                            <m:DatePicker id="_IDGenDatePicker3" value="{path: 'taxModel>ZZ_DATA_A'}" displayFormat="dd.MM.yyyy" valueFormat="yyyy-MM-dd"/>
                        </template>
                    </Column>
                    <Column id="_IDGenColumn3" width="10em" sortProperty="ZZ_TASSO" filterProperty="ZZ_TASSO">
                        <m:Label id="_IDGenLabel3" text="{i18n>ZZ_TASSO}"/>
                        <template>
                            <m:Input id="_IDGenDatePicker4" value="{path: 'taxModel>ZZ_TASSO'}" type="Number"/>
                        </template>
                    </Column>
                </columns>
            </Table>
        </m:content>
        <!--<Button text="{i18n>save}" width="8em" press="onCloseSHdialogEMP"/>-->
        <m:buttons>
            <m:Button id="_IDGenButton1" text="{i18n>save}" type="Emphasized" width="8em" press="onSaveTax"/>
            <m:Button id="_IDGenButton2" text="{i18n>close}" type="Reject" width="8em" press="onCloseTax"/>
        </m:buttons>
    </m:Dialog>
</core:FragmentDefinition>